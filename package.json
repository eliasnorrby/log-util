{
  "name": "@eliasnorrby/log-util",
  "version": "1.1.1",
  "description": "A dead simple log utility",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/**/*"
  ],
  "engines": {
    "node": ">=8"
  },
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "semantic-release": "semantic-release",
    "check-format": "prettier --list-different '**/*.{js,jsx,ts,tsx,html,vue,css,less,scss,graphql,yaml,yml,json,md,mdx}'",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,html,vue,css,less,scss,graphql,yaml,yml,json,md,mdx}'",
    "test": "jest",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eliasnorrby/log-util.git"
  },
  "keywords": [
    "log"
  ],
  "author": "Elias Norrby",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eliasnorrby/log-util/issues"
  },
  "homepage": "https://github.com/eliasnorrby/log-util#readme",
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@eliasnorrby/commitlint-config": "^2.3.8",
    "@eliasnorrby/jest-config": "^2.0.0",
    "@eliasnorrby/prettier-config": "^4.0.0",
    "@eliasnorrby/semantic-release-config": "^3.0.0",
    "@types/jest": "^25.1.4",
    "husky": "^4.0.10",
    "jest": "^25.2.4",
    "prettier": "^2.0.2",
    "semantic-release": "^17.0.4",
    "ts-jest": "^25.3.0",
    "typescript": "^3.7.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "chalk": "^4.0.0"
  }
}
